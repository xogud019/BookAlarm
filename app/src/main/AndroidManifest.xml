<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookalarm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BookAlarm"
        android:usesCleartextTraffic="true">
        <activity android:name=".USERSET.UserModify"></activity>
        <activity android:name=".USERSET.UserPage" />
        <activity android:name=".SETTING.Setting" />
        <activity android:name=".Search.SearchMore" />
        <activity android:name=".Search.SearchResult" />
        <activity android:name=".Search.SearchMain" />
        <activity android:name=".POPUP.PopUp2" />
        <activity android:name=".POPUP.PopUp1" />
        <activity android:name=".LOGIN.Search" />
        <activity android:name=".LOGIN.SignUp" />
        <activity android:name=".LOGIN.LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainPage.Main" />
    </application>

</manifest>